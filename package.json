{
  "name": "Northwind-Demo-Application",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "tsc -p ./public/src/",
    "build:dist": "ngc -p public/tsconfig-dist.json && rollup -c public/rollup-config.js",
    "build:watch": "tsc -p ./public/src/ -w",
    "prep": "npm install && npm upgrade && npm prune && node scripts/copy-dist-files.js",
    "test": "npm run test:server",
    "test:server": "mocha ./app/tests --recursive --timeout 5000 --reporter mochawesome",
    "serve": "nodemon server.js",
    "serve:dist": "node server.js",
    "serve:rel": "NODE_ENV=release npm run serve:dist",
    "start": "concurrently \"npm run build:watch\" \"npm run serve\"",
    "start:rel": "NODE_ENV=release npm run start:dist",
    "start:dist": "npm run build:dist && npm run serve:dist"
  },
  "dependencies": {
    "@angular/animations": "^4.0.2",
    "@angular/common": "^4.0.2",
    "@angular/compiler": "^4.0.2",
    "@angular/compiler-cli": "^4.0.2",
    "@angular/core": "^4.0.2",
    "@angular/forms": "^4.0.2",
    "@angular/http": "^4.0.2",
    "@angular/material": "2.0.0-beta.3",
    "@angular/platform-browser": "^4.0.2",
    "@angular/platform-browser-dynamic": "^4.0.2",
    "@angular/platform-server": "^4.0.2",
    "@angular/router": "^4.0.2",
    "app-root-path": "^2.0.1",
    "bcrypt": "~1.0.2",
    "bluebird": "~3.5.0",
    "body-parser": "^1.17.1",
    "bootstrap": "4.0.0-alpha.6",
    "core-js": "^2.4.1",
    "connect-pg-simple": "^3.1.2",
    "dotenv": "~4.0.0",
    "express": "~4.15.2",
    "express-session": "~1.15.1",
    "hammerjs": "2.0.8",
    "jquery": "~3.1.1",
    "method-override": "~2.3.7",
    "morgan": "1.8.1",
    "helmet": "3.5.0",
    "passport": "0.3.2",
    "passport-local": "~1.0.0",
    "pg-promise": "~5.6.3",
    "rxjs": "~5.2.0",
    "systemjs": "0.20.9",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@types/node": "~7.0.5",
    "rollup": "^0.41.6",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^1.0.1",
    "jasmine": "~2.5.3",
    "typescript": "~2.2.2",
    "browser-sync": "~2.18.8",
    "chai": "~3.5.0",
    "chai-http": "git://github.com/chaijs/chai-http.git#3ea4524",
    "concurrently": "~3.4.0",
    "mocha": "~3.2.0",
    "mochawesome": "~2.0.3",
    "nodemon": "~1.11.0",
    "rewire": "^2.5.2",
    "rimraf": "^2.6.1"
  }
}
